# Application Settings
APP_NAME="AI Guardian"
APP_ENV="development"
APP_KEY="base64:your_app_key"
APP_DEBUG="true"

# Server Settings
SERVER_HOST="0.0.0.0"
SERVER_PORT="8080"

# TensorFlow Settings
TENSORFLOW_MODEL_PATH="/path/to/model"
TENSORFLOW_MODEL_NAME="model_name"

# Database Settings
DB_CONNECTION="sqlite"
DB_HOST="localhost"
DB_PORT="5432"
DB_DATABASE="ai_guardian_db"
DB_USERNAME="your_username"
DB_PASSWORD="your_password"

# Nginx Settings
NGINX_HOST="localhost"
NGINX_PORT="80"
NGINX_SSL_CERT="/path/to/ssl/cert"
NGINX_SSL_KEY="/path/to/ssl/key"

# Logging Settings
LOG_LEVEL="debug"
LOG_FILE="/path/to/log/file.log"

# Security Settings
SECRET_KEY="your_secret_key"
ACCESS_TOKEN_TTL="3600"

# API Settings
API_HOST="localhost"
API_PORT="8081"
API_PREFIX="/api"

# Rust Settings
RUST_LOG="debug"
RUST_BACKTRACE="1"

# Other Settings
CORS_ALLOWED_ORIGINS="*"
CORS_ALLOWED_METHODS="GET,POST,PUT,DELETE"
CORS_ALLOWED_HEADERS="Content-Type,Authorization"
